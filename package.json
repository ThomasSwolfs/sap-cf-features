  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.35",
    "@types/sap__xsenv": "^2.0.2",
    "typedoc": "^0.20.34",
    "typedoc-plugin-markdown": "^3.6.0",
    "typescript": "^4.2.3"
  }
